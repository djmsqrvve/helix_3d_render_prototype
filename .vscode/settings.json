{
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', 'Cascadia Code', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.lineHeight": 1.6,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 100,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.js": "${capture}.test.js, ${capture}.spec.js",
        "*.jsx": "${capture}.test.jsx, ${capture}.module.css",
        "*.ts": "${capture}.test.ts, ${capture}.spec.ts, ${capture}.d.ts",
        "*.tsx": "${capture}.test.tsx, ${capture}.stories.tsx, ${capture}.module.css",
        ".env": ".env.*, .envrc",
        "README.md": "LICENSE, CHANGELOG.md, CONTRIBUTING.md, CODE_OF_CONDUCT.md",
        "docker-compose.yml": "docker-compose.*.yml, Dockerfile",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb"
    },
    "files.eol": "\n",
    "files.exclude": {
        "**/*.egg-info": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.next": true,
        "**/.pytest_cache": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/build": true,
        "**/coverage": true,
        "**/dist": true,
        "**/node_modules": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.venv/**": true,
        "**/__pycache__/**": true,
        "**/build/**": true,
        "**/dist/**": true,
        "**/node_modules/*/**": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.path": "/usr/bin/git",
    "javascript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "remote.WSL.fileWatcher.polling": true,
    "remote.WSL.useShellEnvironment": true,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.cargo.features": [
        "egui"
    ],
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.hover.actions.references.enable": true,
    "rust-analyzer.lens.references.adt.enable": true,
    "rust-analyzer.lens.references.enumVariant.enable": true,
    "rust-analyzer.lens.references.method.enable": true,
    "rust-analyzer.lens.references.trait.enable": true,
    "search.exclude": {
        "**/*.egg-info": true,
        "**/.git": true,
        "**/__pycache__": true,
        "**/build": true,
        "**/dist": true,
        "**/node_modules": true
    },
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.defaultProfile.windows": "WSL",
    "terminal.integrated.fontFamily": "'JetBrains Mono', 'Fira Code', monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.profiles.windows": {
        "WSL": {
            "args": [],
            "color": "terminal.ansiGreen",
            "icon": "terminal-linux",
            "path": "C:\\Windows\\System32\\wsl.exe"
        },
        "WSL-Sudo": {
            "args": [
                "-u",
                "root"
            ],
            "color": "terminal.ansiRed",
            "icon": "terminal-linux",
            "path": "C:\\Windows\\System32\\wsl.exe"
        }
    },
    "terminal.integrated.shellIntegration.enabled": true,
    "typescript.suggest.autoImports": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#00ff88",
        "activityBar.background": "#050806",
        "activityBar.foreground": "#d4e8dc",
        "activityBar.inactiveForeground": "#5a7a6a",
        "badge.background": "#00ff88",
        "badge.foreground": "#050806",
        "button.background": "#2d4a3e",
        "button.foreground": "#d4e8dc",
        "button.hoverBackground": "#3d5a4e",
        "dropdown.background": "#0a0f0d",
        "dropdown.border": "#2d4a3e",
        "editor.background": "#0a0f0d",
        "editor.foreground": "#d4e8dc",
        "editor.lineHighlightBackground": "#1a2f25",
        "editor.selectionBackground": "#2d4a3e",
        "editorCursor.foreground": "#ff5e99",
        "editorLineNumber.activeForeground": "#00ff88",
        "editorLineNumber.foreground": "#3a5a4a",
        "focusBorder": "#00ff88",
        "foreground": "#d4e8dc",
        "input.background": "#050806",
        "input.border": "#2d4a3e",
        "input.placeholderForeground": "#5a7a6a",
        "list.activeSelectionBackground": "#2d4a3e",
        "list.focusBackground": "#2d4a3e",
        "list.focusForeground": "#d4e8dc",
        "list.highlightForeground": "#00ff88",
        "list.hoverBackground": "#152018",
        "menu.background": "#0a0f0d",
        "menu.foreground": "#d4e8dc",
        "menu.selectionBackground": "#2d4a3e",
        "menu.selectionBorder": "#00ff88",
        "menu.selectionForeground": "#d4e8dc",
        "menu.separatorBackground": "#1a2f25",
        "panel.background": "#050806",
        "panel.border": "#1a2f25",
        "quickInput.background": "#050806",
        "quickInput.foreground": "#d4e8dc",
        "quickInputList.focusBackground": "#2d4a3e",
        "quickInputList.focusForeground": "#d4e8dc",
        "scrollbarSlider.background": "#2d4a3e50",
        "scrollbarSlider.hoverBackground": "#3d5a4e",
        "sideBar.background": "#050806",
        "sideBar.foreground": "#b8d0c4",
        "sideBarSectionHeader.background": "#0a0f0d",
        "sideBarSectionHeader.foreground": "#d4e8dc",
        "statusBar.background": "#1a2f25",
        "statusBar.debuggingBackground": "#ff5e99",
        "statusBar.debuggingForeground": "#0a0f0d",
        "statusBar.foreground": "#00d4aa",
        "statusBar.noFolderBackground": "#1a2f25",
        "tab.activeBackground": "#0a0f0d",
        "tab.activeBorder": "#00ff88",
        "tab.activeForeground": "#d4e8dc",
        "tab.hoverBackground": "#152018",
        "tab.inactiveBackground": "#050806",
        "tab.inactiveForeground": "#6a8a7a",
        "terminal.ansiBlue": "#3399ee",
        "terminal.ansiBrightBlue": "#44aaff",
        "terminal.ansiBrightCyan": "#00d4aa",
        "terminal.ansiBrightGreen": "#00ff88",
        "terminal.ansiBrightMagenta": "#ff5e99",
        "terminal.ansiBrightRed": "#ff5577",
        "terminal.ansiBrightYellow": "#ffee55",
        "terminal.ansiCyan": "#00b494",
        "terminal.ansiGreen": "#00dd77",
        "terminal.ansiMagenta": "#e64d82",
        "terminal.ansiRed": "#ee4466",
        "terminal.ansiYellow": "#e6cc40",
        "terminal.background": "#050806",
        "terminal.foreground": "#d4e8dc",
        "titleBar.activeBackground": "#0a0f0d",
        "titleBar.inactiveBackground": "#050806",
        "titleBar.inactiveForeground": "#5a7a6a"
    },
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.editor.showTabs": "multiple",
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "welcomePage",
    "workbench.tree.indent": 16
}
